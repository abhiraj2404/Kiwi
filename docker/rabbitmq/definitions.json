{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "guest",
      "password_hash": "guest",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "rabbit@rabbitmq"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "x402-batch-queue-policy",
      "pattern": "^x402-batch-.*",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic",
        "message-ttl": 3600000,
        "max-length": 10000
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "x402-batch-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000,
        "x-overflow": "reject-publish"
      }
    },
    {
      "name": "x402-reconciliation-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 1000
      }
    }
  ],
  "exchanges": [
    {
      "name": "x402-batch-exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "x402-reconciliation-exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "x402-batch-exchange",
      "vhost": "/",
      "destination": "x402-batch-queue",
      "destination_type": "queue",
      "routing_key": "batch.process",
      "arguments": {}
    },
    {
      "source": "x402-reconciliation-exchange",
      "vhost": "/",
      "destination": "x402-reconciliation-queue",
      "destination_type": "queue",
      "routing_key": "reconcile.process",
      "arguments": {}
    }
  ]
}
